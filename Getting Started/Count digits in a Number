import java.util.*;
  
  public class Main{
  
  public static void main(String[] args) {
    Scanner scn = new Scanner(System.in);
    int n = scn.nextInt();  // n= 123
    int count = 0;

    while(n!=0) {
    int rem = n%10;  //rem = 3
    n= n/10;         //n = 12
    count+=1;
                }
                
    System.out.println(count);
   }
  }
